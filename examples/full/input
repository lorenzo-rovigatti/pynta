filename = "mycode.c"

[compilation]
command = "gcc"
all_warnings = true
all_warnings_command = "gcc -Wall"

[execution]
arguments = ""
stdin = """
1
2 3
"""

[valgrind]
enable = true
command = "valgrind --leak-check=full --show-leak-kinds=all"
xml_file = "valgrind_log.xml"

[output]
files = ["output.dat"]
