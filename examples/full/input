filename = "mycode.c"
working_dir = "pynta_output"

[compilation]
report_path = "compilation_report.txt"
command = "gcc"
all_warnings = true
all_warnings_command = "gcc -Wall"

[execution]
report_path = "execution_report.txt"
arguments = ""
stdin = """
1
2 3
"""
expected_return_code = 0

[valgrind]
enable = true
command = "valgrind --leak-check=full --show-leak-kinds=all"
xml_file = "valgrind_log.xml"

output_report_path = "output_report.txt"

[[output]]
type = "stdout"
equal_to = "../correct_stdout.dat"

[[output]]
type = "stderr"
empty = true

[[output]]
type = "file"
name = "output.dat"
